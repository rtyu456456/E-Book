<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fp.eb.mapper.TradeMapper">

	<select id="getAlltradelist" resultType="com.fp.eb.model.TradeDTO">
		select * from eb_trade
	</select>
	
	<select id="getTradeDetail" resultType="com.fp.eb.model.TradeDTO" >
	select * from eb_trade where t_no = #{t_no }
	</select>
	
	<select id="getSearchReg" resultType="com.fp.eb.model.TradeDTO"></select>

</mapper>